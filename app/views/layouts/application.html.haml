!!!
%html

  %head
    %title
      - if content_for? :title
        = yield(:title)
        = " - "
      New Application
    %meta{"http-equiv"=>"Content-Type", :content=>"text/html; charset=utf-8"}
    %meta{name: "viewport", content: "width=device-width, initial-scale=1.0"}
    %link{rel: "apple-touch-icon", href: "/ios/AppIcon.appiconset/Icon-40@2x.png", sizes: "80x80"}
    %link{rel: "apple-touch-icon", href: "/ios/AppIcon.appiconset/Icon-60@2x.png", sizes: "120x120"}
    %link{rel: "apple-touch-icon", href: "/ios/AppIcon.appiconset/Icon-60@3x.png", sizes: "180x180"}
    %link{rel: "apple-touch-icon", href: "/ios/AppIcon.appiconset/Icon-76.png", sizes: "76x76"}
    %link{rel: "apple-touch-icon", href: "/ios/AppIcon.appiconset/Icon-76@2x.png", sizes: "152x152"}
    = stylesheet_link_tag "application"
    = javascript_include_tag "application"
    = csrf_meta_tag
    = favicon_link_tag
    = yield(:head)
    = analytics_init if Rails.env.production?

  %body
    = yield
